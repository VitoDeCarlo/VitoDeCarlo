    // Layers
    {
      "name": "layers",
      "Layers": [
        {
          "Name": "Always",
          "LayerRule": {
            "ConditionId": "[js: uuid()]",
            "Conditions": [
              {
                "$type": "OrchardCore.Rules.Models.BooleanCondition, OrchardCore.Rules",
                "Name": "BooleanCondition",
                "Value": true,
                "ConditionId": "[js: uuid()]"
              }
            ]
          },
          "Description": "The widgets in this layer are displayed on any page of this site."
        },
        {
          "Name": "Homepage",
          "LayerRule": {
            "ConditionId": "[js: uuid()]",
            "Conditions": [
              {
                "$type": "OrchardCore.Rules.Models.HomepageCondition, OrchardCore.Rules",
                "Name": "HomepageCondition",
                "Value": true,
                "ConditionId": "[js: uuid()]"
              }
            ]
          },
          "Description": "The widgets in this layer are only displayed on the homepage."
        },
        {
          "Name": "NotHome",
          "LayerRule": {
            "ConditionId": "[js: uuid()]",
            "Conditions": [
              {
                "$type": "OrchardCore.Rules.Models.HomepageCondition, OrchardCore.Rules",
                "Name": "HomepageCondition",
                "Value": false,
                "ConditionId": "[js: uuid()]"
              }
            ]
          },
          "Description": "The widgets in this layer are displayed anywhere but the Home Page."
        }
      ]
    },
