{
  "name": "Vito",
  "displayName": "Vito",
  "description": "Custom theme for Vito DeCarlo's personal website.",
  "author": "thePulse Inc.",
  "website": "https://thepulse.net",
  "version": "1.0.0",
  "issetuprecipe": true,
  "categories": [ "default" ],
  "tags": [ "vito" ],

  // The variables are evaluated the first time they are accessed, and reused across steps
  "variables": {
    "landingContentItemId": "[js:uuid()]",
    "galleryContentItemId": "[js:uuid()]",
    "categoriesContentItemId": "[js:uuid()]",
    "landscapesCategoryContentItemId": "[js:uuid()]",
    "cityscapesCategoryContentItemId": "[js:uuid()]",
    "tagsContentItemId": "[js:uuid()]",
    "orlandoTagContentItemId": "[js:uuid()]",
    "tampaTagContentItemId": "[js:uuid()]",
    "uticaTagContentItemId": "[js:uuid()]",
    "clermontTagContentItemId": "[js:uuid()]",
    "floridaTagContentItemId": "[js:uuid()]",
    "sunsetTagContentItemId": "[js:uuid()]",
    "lakeTagContentItemId": "[js:uuid()]",
    "fogTagContentItemId": "[js:uuid()]",
    "sandiegoTagContentItemId": "[js:uuid()]",
    "californiaTagContentItemId": "[js:uuid()]",
    "beachTagContentItemId": "[js:uuid()]",
    "newyorkTagContentItemId": "[js:uuid()]",
    "now": "[js: new Date().toISOString()]"
